// This file defines the FGPU-specific builtin function database. Users of
// this file must define the BUILTIN macro to make use of this information.
//
//===----------------------------------------------------------------------===//

// The format of this database matches clang/Basic/Builtins.def.

BUILTIN(__builtin_fgpu_lid, "ii", "nc")
BUILTIN(__builtin_fgpu_size, "ii", "nc")
BUILTIN(__builtin_fgpu_wgid, "ii", "nc")
BUILTIN(__builtin_fgpu_wgsize, "ii", "nc")
BUILTIN(__builtin_fgpu_wgoff, "ii", "nc")

//TODO: also add in the input for the accumulator since it is inout
BUILTIN(__builtin_fgpu_dot, "ffV32fV32f*1i", "nc") // TODO: check address space

BUILTIN(__builtin_fgpu_vdotm0, "ffV32fV32f*1i", "nc") // TODO: check address space
BUILTIN(__builtin_fgpu_vdotm1, "ffV32fV32f*1i", "nc") // TODO: check address space

BUILTIN(__builtin_fgpu_movc, "V32fV32fii", "nc") // TODO: check address space



BUILTIN(__builtin_fgpu_lsync, "v", "")
BUILTIN(__builtin_fgpu_gsync, "v", "")


//BUILTIN(__builtin_msa_xori_b, "V16cV16cIUi", "nc")

#undef BUILTIN
